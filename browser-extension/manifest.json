{
  "description": "Allow only websites in allowed list for a period of time",
  "manifest_version": 2,
  "name": "FocusOne",
  "version": "1.0",
  "icons": {
    "48": "icons/message.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "focusone@.org",
      "strict_min_version": "50.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/message.svg"
  },
  "permissions": [
    "nativeMessaging"
  ]
}
