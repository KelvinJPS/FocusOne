{
  "manifest_version": 2,
  "name": "FocusOne",
  "version": "0.1",
  "description": "Block websites not in allowed list for a period of time",
  "background": {
    "scripts": ["background.js"]
  }, 
  "permissions": ["nativeMessaging"],
  "browser_specific_settings": {
    "gecko": {
      "id": "FocusOne@xyz",
      "strict_min_version": "50.0"
    }
  }
}

